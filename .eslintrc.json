{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": 0,
        "no-shadow": 0,
        "consistent-return": 0,
        "no-else-return": 0,
        "lines-between-class-members": 0,
        "no-underscore-dangle": 0
    }
}